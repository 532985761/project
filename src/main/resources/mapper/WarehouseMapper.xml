<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.WarehouseMapper">

    <resultMap id="BaseResultMap" type="com.example.demo.domain.Warehouse">
            <id property="warehouseId" column="warehouse_id" jdbcType="INTEGER"/>
            <result property="warehouseName" column="warehouse_name" jdbcType="VARCHAR"/>
            <result property="creatTime" column="creat_time" jdbcType="TIMESTAMP"/>
            <result property="rentTime" column="rent_time" jdbcType="INTEGER"/>
            <result property="area" column="area" jdbcType="INTEGER"/>
            <result property="userId" column="user_id" jdbcType="INTEGER"/>
            <result property="notice" column="notice" jdbcType="VARCHAR"/>
            <result property="status" column="status" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        warehouse_id,warehouse_name,creat_time,
        rent_time,area,user_id,
        notice,status
    </sql>
</mapper>
